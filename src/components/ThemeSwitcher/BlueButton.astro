	.button-container {
		position: relative;
		width: 120px;
		height: 120px;
	}

	svg {
		position: absolute;
		top: 0;
		left: 0;
		width: 100%;
		height: 100%;
		transition: opacity 0.2s ease;
	}

	svg.regular {
		opacity: 1;
	}

	svg.hover {
		opacity: 0;
	}

	.button-container:hover svg.regular {
		opacity: 0;
	}

	.button-container:hover svg.hover {
		opacity: 1;
	}
</style>
